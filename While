using System.Timers;
using static System.Runtime.InteropServices.JavaScript.JSType;

namespace Devam
{
    internal class Program
    {

        static void Main(string[] args)
        {
            int sayi = 0;
            while (sayi <10)
            {
                Console.WriteLine("isim gir: ");
                Console.ReadLine();
                sayi++;
                Console.WriteLine("şu kadar kişi oldu " + sayi);
            }
            Console.WriteLine("10 kişi yeterli");
        }

        }
       
        }
        
    

